#include<iostream>
//继承 class name: 继承方式 父类1, 继承方式 父类2
class A
{
public:
    int a;
	A()
	{
	    a = 10;
	}

};
class B
{
public: 
    int b;
	B()
	{
	    b = 20;
	 	std::cout << "constructor\n";
	}
};
class C:public A ,public B
{
public:
    C()
	{
	    std::cout << "C\n";
	}
};
int main()
{
    C c;
	std::cout << c.a << c.b << "\n";

	return 0;
}
